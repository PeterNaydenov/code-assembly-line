                    "c8": "^9.1.0",
                    "chai": "5.0.3",
                    "mocha": "10.2.0",
                    "rollup": "^4.9.6"
          },
          "c8": {
                    "include": [
                              "src/**/*.js"
                    ],
                    "exclude": [
                              "node_modules",
